  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "@qbit/auth/(.*)": "<rootDir>/libs/auth/src/$1",
      "@qbit/auth": "<rootDir>/libs/auth/src",
      "@qbit/appVersion/(.*)": "<rootDir>/libs/appVersion/src/$1",
      "@qbit/appVersion": "<rootDir>/libs/appVersion/src",
      "@qbit/news/(.*)": "<rootDir>/libs/news/src/$1",
      "@qbit/news": "<rootDir>/libs/news/src",
      "@qbit/config/(.*)": "<rootDir>/libs/config/src/$1",
      "@qbit/config": "<rootDir>/libs/config/src",
      "@qbit/ads/(.*)": "<rootDir>/libs/ads/src/$1",
      "@qbit/ads": "<rootDir>/libs/ads/src",
      "@qbit/appContract/(.*)": "<rootDir>/libs/appContract/$1",
      "@qbit/appContract": "<rootDir>/libs/appContract",
      "@qbit/notification/(.*)": "<rootDir>/libs/notification/src/$1",
      "@qbit/notification": "<rootDir>/libs/notification/src",
      "@qbit/forest-admin/(.*)": "<rootDir>/libs/forest-admin/src/$1",
      "@qbit/forest-admin": "<rootDir>/libs/forest-admin/src",
      "@qbit/uploader/(.*)": "<rootDir>/libs/uploader/src/$1",
      "@qbit/uploader": "<rootDir>/libs/uploader/src",
      "@qbit/firebase/(.*)": "<rootDir>/libs/firebase/src/$1",
      "@qbit/firebase": "<rootDir>/libs/firebase/src",
      "@qbit/genesis-forum/(.*)": "<rootDir>/libs/genesis-forum/src/$1",
      "@qbit/genesis-forum": "<rootDir>/libs/forest-admin/src",
      "@qbit/xendit/(.*)": "<rootDir>/libs/xendit/src/$1",
      "@qbit/xendit": "<rootDir>/libs/xendit/src"
    }
